            <div>
              <!-- Status Icon: RadialProgressWrapper = in progress -->
              <figure class="el6aqu80 gamut-hgc6w6-RadialProgressWrapper">
                <figcaption>60% progress</figcaption>
                <svg><!-- Progress circle SVG --></svg>
              </figure>
              
              <h2>Database Basics</h2>
